<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
    <RootNamespace>GoVFlow</RootNamespace>
    <Authors>BenDerPan</Authors>
    <Company>BenDerPan</Company>
    <Product>BenDerPan</Product>
    <Description>GoXAML version of VFlow.</Description>
    <Copyright>https://github.com/BenDerPan/VFlow</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageIcon>vflow.png</PackageIcon>
    <RepositoryUrl>https://github.com/BenDerPan/VFlow</RepositoryUrl>
    <PackageProjectUrl>https://github.com/BenDerPan/VFlow</PackageProjectUrl>
    <RepositoryType>github</RepositoryType>
    <PackageTags>flow based workflow programming WPF</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <None Include="vflow.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Northwoods.GoWPF" Version="3.0.1" />
  </ItemGroup>

</Project>
